include /etc/nginx/zulip-include/headers;
add_header Access-Control-Allow-Origin 'https://dash.anonydoxx.network' always;
add_header Access-Control-Allow-Headers Authorization always;
add_header Access-Control-Allow-Credentials true always;
add_header Access-Control-Allow-Headers 'access-control-allow-credentials' always;
add_header Access-Control-Allow-Methods 'GET, POST, DELETE, PUT, PATCH, HEAD' always;
